  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/EBAC-QE/ci-cd-teste-exemplo/issues"
  },
  "homepage": "https://github.com/EBAC-QE/ci-cd-teste-exemplo#readme",
  "dependencies": {
    "cypress": "^14.5.4"
  },
  "devDependencies": {
    "allure-commandline": "^2.34.1",
    "allure-cypress": "^3.4.2",
    "mocha-allure-reporter": "^1.4.0"
  }
}
