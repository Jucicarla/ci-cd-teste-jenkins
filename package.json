{
  "name": "ci-cd-teste-exemplo",
  "version": "1.0.0",
  "description": "Projeto simples para aulas de CI-CD",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run --browser chrome",
    "report": "npx allure serve --clean allure-results",
    "test-chrome": "npx cypress run --browser chrome",
    "teste-suite-1": "npx cypress run --spec \"cypress/e2e/1-getting-started/*\"",
    "teste-suite-2": "npx cypress run --browser chrome --spec \"cypress/e2e/2-advanced-examples/*\"",
    "teste-suite-3": "npx cypress run --spec \"cypress/e2e/3-paralelo/*\"",
    "allure": "npx allure serve allure-results"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EBAC-QE/ci-cd-teste-exemplo.git"
  },
  "keywords": [],
